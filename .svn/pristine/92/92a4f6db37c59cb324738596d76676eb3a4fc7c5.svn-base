<script type="text/javascript">
    function changeCurrency(currency_id) {
        $.ajax({
            type:'POST',
            url:'{base_url('login/change-currency')}',
            data: { currency_id: currency_id },
            dataType:'json',
        })
        .done(function( html ) {
            window.location.reload();
        });
    }
</script>
<script type="text/javascript">

    $(document).on("click",".add-to-cart",function() {
        if($(this).hasClass('disabled')){
            return false;
        }
        var base_url = $('#rootPath').val();
        var productID = $(this).attr('data-productID');
        var size = $('input[name="dimension"]:checked').val();
        var color = $('input[name="color"]:checked').val();
        qty = $('#product-quantity1').val();
        if( productID ){                  
            $.ajax({
                type:'POST',
                url:base_url+'user/inventory/addToCart',
                data: { quantity:qty,product_id:productID,size:size,color:color} ,
                dataType:'json',

            })
            .done(function( html ) {
                if(!html.status){ 
                   $.notify(
                    html.msg, 
                    { color: "", background: "",blur: 0.4, delay:4000, icon: "fa fa-exclamation-circle",type:"danger"}
                    );
               }else{  

                console.log(html);
                var cart_total = html.cart_total;
                $('.cart-total').html(cart_total); 
                $(".shopping-cart").html(html.cart_contents)
                $.notify(
                    "Item Successfully added to your cart!", 
                    { color: "", background: "",blur: 0.4, delay:4000, icon: "fa fa-check",type:"success"}
                    );
            } 

        });
        }
    });
    $(document).on("click",".add-to-whishlist",function() {
        var base_url = $('#rootPath').val();
        var productID = $(this).attr('data-productID');
        var size = $('input[name="dimension"]:checked').val();
        var color = $('input[name="color"]:checked').val();
// console.log(size);   
        // if(empty(size))
        // {
        // var size = $(this).attr('data-defaultColorID');
        // }
        // var defaultDimensionID = $(this).attr('data-defaultDimensionID');
    // alert(color);
        if( productID ){
         $.ajax({
             type:'POST',
             url:base_url+'user/inventory/addToWhishlist',
             data: { product_id:productID,size:size,color:color} ,
             dataType:'json',
         })
         .done(function( html ) {

            if(!html.status){ 
                $.notify(
                    html.msg, 
                    { color: "", background: "",blur: 0.4, delay:4000, icon: "fa fa-exclamation-circle",type:"danger"}
                    );

            }else{ 
                var wishlist_total = html.wishlist_total;
                $('.whishlist-total').html(wishlist_total); 
                $(".wishlist-content").html(html.wishlist_contents);
                $.notify(
                    "Item Successfully added in wishlist", 
                    { color: "", background: "",blur: 0.4, delay:4000, icon: "fa fa-check",type:"success"}
                    );
            } 

        });
     }
 });

    $('body').on('click', '.remove-wishlist', function() {
        var row=$(this).attr('data-row_id');
        $.ajax({
            type:'POST',
            url:'{base_url("user/inventory/Removewishlist")}',
            data: { delete: row } ,
            dataType:'json',
        })
        .done(function( response ) { 
            if(!response.status){
                toastr.error(response.msg, "Failed: ",{ progressBar:!0 })
            }else{
                $(".whishlist-total").html(response.wishlist_total)
                $(".shopping-wishlist").html(response.wishlist_contents)
                $('#'+row).remove();
                $.notify(
                    "Item Successfully removed from wishlist", 
                    { color: "", background: "",blur: 0.4, delay:4000, icon: "fa fa-check",type:"success"}
                    );
                location.reload();
            }
        })
    });

    $(document).on("click","#messageDropdown",function() {

        $.ajax({
            type:'POST',
            url:'{base_url()}'+'{log_user_type()}'+'/autocomplete/get-user-message',
            dataType:'json',
            beforeSend: function() {
                $('.list-group-item').hide();
                $(".loading").delay(3).fadeIn("slow");
            },
        })
        .done(function(response) { 
            if (response.success) {
                $(".notif").html(response.recent_message)
            }               
            $(".loading").delay(3).fadeOut("slow");

        });
    });

</script>


